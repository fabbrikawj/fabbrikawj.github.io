    <script>
        removeActiveVoice();

        var url = window.location.pathname.substring(7);
        url = url.indexOf('/') > 0 ? url.substring(0, url.indexOf('/')).toLowerCase() : url;
        var hrefUrlComplete = getPathOfUrl(window.location.href).toLowerCase();

        var refer = getPathOfUrl(document.referrer).substring(7);
        refer = refer.indexOf('/') > 0 ? refer.substring(0, refer.indexOf('/')).toLowerCase() : refer;
        var referUrlComplete = getPathOfUrl(document.referrer).toLowerCase();

        var urlNotInMenu = getPathOfUrlNotInMenu(window.location.href).toLowerCase();
        var urlNotInMenuFirstLevel = getPathOfUrlNotInMenuFirstLevel(window.location.href).toLowerCase();

        jQuery("#header .os-navigation li.level0").each(function() {
            var hrefMenu = jQuery(this).find("ul.level0 li.level1");
            var hrefToActive = jQuery(this);
            //first search: menu url
            var find = searchActiveVoiceInMenu(hrefMenu, hrefToActive, hrefUrlComplete);
            //second search: url not present in menu. Use refer
            if (!find){
                find = searchActiveVoiceInMenu(hrefMenu, hrefToActive, referUrlComplete);
            }
            //third search: url not present in menu and empty refer
            if (!find){
                find = searchActiveVoiceInMenu(hrefMenu, hrefToActive, urlNotInMenu);
            }
            //third search: url not present in menu and empty refer
            if (!find){
                searchActiveVoiceInMenu(hrefMenu, hrefToActive, urlNotInMenuFirstLevel);
            }
        });

        function searchActiveVoiceInMenu(hrefMenu, hrefToActive, hrefUrlToCheck) {
            var find = false;
            hrefMenu.each(function() {
                var subMenuA = jQuery(this).find("a");
                var hrefSubMenu = jQuery(this).find("a").attr("href");
                hrefSubMenu = getPathOfUrl(hrefSubMenu).toLowerCase();
                //TODO: gestire una soluzione migliore (url !== "video") per questa casistica
                if(hrefSubMenu.indexOf(url) > 0 && url !== "video"){
                    hrefToActive.addClass("active");
                    if(hrefUrlToCheck === hrefSubMenu){
                        subMenuA.addClass("active");
                        find = true;
                    }
                }
            });
            return find;
        }

        function getPathOfUrl(url) {
            var urlRet = "";
            url = url.split( '/' );
            for (var i = 3; i<url.length; i++) {
                urlRet = urlRet + "/" + url[i];
            }
            return urlRet;
        }

        function getPathOfUrlNotInMenu(url) {
            var urlRet = "";
            url = url.split( '/' );
            for (var i = 3; i<6; i++) {
                urlRet = urlRet + "/" + url[i];
            }
            return urlRet;
        }

        function getPathOfUrlNotInMenuFirstLevel(url) {
            var urlRet = "";
            url = url.split( '/' );
            for (var i = 3; i<5; i++) {
                urlRet = urlRet + "/" + url[i];
            }
            return urlRet;
        }

        function removeActiveVoice() {
            jQuery("#header .os-navigation li.level0").each(function() {
                var hrefMenu = jQuery(this).find("ul.level0 li.level1");
                var hrefToActive = jQuery(this);
                hrefMenu.each(function() {
                    var subMenuA = jQuery(this).find("a");
                    hrefToActive.removeClass("active");
                    subMenuA.removeClass("active");
                });
            });
        }
    </script>

